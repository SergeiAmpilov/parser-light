<h1>Sitemap by task - {{ taskid }}</h1>

{{#if list.length}}
 <ul>
    {{#each list}}
      <li>
        id: {{ id }}
        <br>
        url: {{ url }}
        <br>
        lastmod: {{ lastmod }}
        <br>
        <a href="/seoparser/renderpages?sitemap={{url}}&taskid={{taskid}}">view pages</a>
      </li>

      <hr>
    
    {{/each}}
  </ul>
{{/if}}
<a href="/seoparser/render">go back to seoparser</a>